package ProgrammingBasicswithJava_Jan2022.Exercise.Ex_04_For_Loop;


public class E04_01_Numbers_Ending_in_7 {
    public static void main(String[] args) {


        for (int i = 1; i <= 1000; i++) {

            if (i % 10 == 7) {
                System.out.println(i);
            }
        }
    }
}
